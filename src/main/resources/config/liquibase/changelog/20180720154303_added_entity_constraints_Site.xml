<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Site.
    -->
    <changeSet id="20180720154303-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="palette_id"
                                 baseTableName="site"
                                 constraintName="fk_site_palette_id"
                                 referencedColumnNames="id"
                                 referencedTableName="site_color_palette"/>

        <addForeignKeyConstraint baseColumnNames="footer_id"
                                 baseTableName="site"
                                 constraintName="fk_site_footer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="site_footer"/>

    </changeSet>
</databaseChangeLog>
